{
  "expo": {
    "name": "MaxiBreak",
    "slug": "maxibreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1b36c5e6-cd19-4ce7-b756-fbfbd891e72c"
      }
    },
    "owner": "akash_ramasani",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": "node_modules/@expo-google-fonts/roboto-slab"
        }
      ]
    ]
  }
}
